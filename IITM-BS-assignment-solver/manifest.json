{
    "manifest_version": 2,
    "name": "IITM Practice Assignment Solver",
    "description": "This extension will solve the practice assignment by IITM BS degree",
    "version": "1.0",
    "permissions": ["activeTab"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  